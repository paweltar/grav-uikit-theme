<ul class="uk-list uk-link-reset">
{% for month,items in archives_data %}
    <li>
      <span class="uk-margin-small-right" uk-icon="icon: chevron-right"></span>
    	<a href="{{ base_url }}/{{ config.plugins.archives.taxonomy_names.month }}{{ config.system.param_sep }}{{ date|date('M_Y')|lower|e('url') }}">
        {% if archives_show_count %}
        <span>({{ items|length }})</span>
        {% endif %}
        <span class="archive_date">{{ month }}</span>
      </a>
    </li>
{% endfor %}
</ul>
